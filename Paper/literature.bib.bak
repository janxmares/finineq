@Article{Ranciereetal2006,
  Title                    = {Decomposing the effects of financial liberalization: Crises vs. growth},
  Author                   = {Ranciere, Romain and Tornell, Aaron and Westermann, Frank},
  Journal                  = {Journal of Banking \& Finance},
  Year                     = {2006},
