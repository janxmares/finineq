@Article{Abiadetal2008,
  Title                    = {{A new database of financial reforms}},
  Author                   = {Abdul Abiad and Enrica Detragiache and Thierry Tressel},
  Journal                  = {IMF Staff Papers},
  Year                     = {2010},
  Number                   = {2},
  Pages                    = {281-302},
  Volume                   = {57},

  Abstract                 = {This paper introduces a new database of financial reforms covering 91 economies over 1973–2005. It describes the content of the database, the information sources utilized, and the coding rules used to create an index of financial reform. It also compares the database with other measures of financial liberalization, provides descriptive statistics, and discusses some possible applications. The database provides a multifaceted measure of reform, covering seven aspects of financial sector policy. Along each dimension the database provides a graded (rather than a binary) score, and allows for reversals.},
  Url                      = {http://ideas.repec.org/a/pal/imfstp/v57y2010i2p281-302.html}
}

