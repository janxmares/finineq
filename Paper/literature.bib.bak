@Article{DemirgucMaksimovic1998,
  Title                    = {Law, Finance, and Firm Growth.},
  Author                   = {Demirguküçl-Kunt, Asli and Maksimovic, Vojislav},
  Journal                  = {Journal of Finance},
  Year                     = {1998},
  Number                   = {6},
  Pages                    = {2107-2137},
  Volume                   = {53},

  Abstract                 = {We investigate how differences in legal and financial systems affect firms' use of external financing to fund growth. We show that in countries whose legal systems score high on an efficiency index, a greater proportion of firms use long-term external financing. An active, though not necessarily large, stock market and a large banking sector are also associated with externally financed firm growth. The increased reliance on external financing occurs in part because established firms in countries with well-functioning institutions have lower profit rates. Government subsidies to industry do not increase the proportion of firms relying on external financing. [ABSTRACT FROM AUTHOR]},
